Model order_details {
  type: 'query'
  label: 'order_details'
  description: ''
  data_source_name: 'khaipostgresql'
  dimension email {
    label: 'Email'
    type: 'text'
    hidden: false
    definition: @sql {{ #SOURCE.email }};;
  }
  dimension id {
    label: 'Id'
    type: 'number'
    hidden: false
    definition: @sql {{ #SOURCE.id }};;
  }
  dimension status {
    label: 'Status'
    type: 'text'
    hidden: false
    definition: @sql {{ #SOURCE.status }};;
  }

  owner: 'khai.to@holistics.io'
  query: @sql
    select u.email, o.id, o.status
    from ecommerce.orders o
      left join ecommerce.users u on o.user_id = u.id;;
  models: [
  ]
}
