// import './users.model.aml' { users }

Model orders {
  type: 'table'
  label: "Orders"
  table_name: 'ecommerce.orders'
  data_source_name: 'demodb'
  description: "orders model description"

  dimension id {
    label: 'Id'
    type: 'number'
  }

  dimension status {
    label: 'Status'
    type: 'text'
  }

  dimension created_at {
    label: 'Created At'
    type: 'datetime'
  }

  dimension user_id {
    label: 'User Id'
    type: 'number'
  }

  dimension discount {
    label: 'Discount'
    type: 'number'
  }

  dimension delivery_attempts {
    label: 'Delivery Attempts'
    type: 'number'
  }
  
}

Relationship rel_a {
  type: 'many_to_one'
  from: ref('orders', 'user_id')
  to: ref('users', 'id')
}

// users
orders